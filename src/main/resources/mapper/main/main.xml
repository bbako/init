<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mainMapper">
    <select id="getMember" resultType="MainVO">
        SELECT
            mem_seq
            , mem_id
            , mem_pw
            , mem_name
        FROM member;
    </select>
</mapper>